public class Main
{

    public static void main(String[] args)
    {
        int number = 5;

        for (int i=2; i<number; i++)
        {
            if (number % i != 0)
            {
                System.out.printl ("Your number is a prime number")
            }
            else
            {
                System.out.printl ("Your number is not a prime number")
            }
        }
    }
}
